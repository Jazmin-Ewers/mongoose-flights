<form id="new-form" action="/tickets" method="POST">
    <label>Seat:</label>
    <input name="seat" required pattern="[A-F][1-9]\d?">
    <label>Price:</label>
    <input type="number" min="0" step="10">
    <input type="submit" value="Add Flight">
  </form>

